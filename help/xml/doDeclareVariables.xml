    <blocks>
        <block-definition s="swap items %'index1' and %'index2' in %'list'" type="command" category="lists">
            <header />
            <code />
            <translations />
            <inputs>
                <input type="%n" />
                <input type="%n" />
                <input type="%l" />
            </inputs>
            <script>
                <block s="doDeclareVariables" annotation="1">
                    <list annotation="2">
                        <l>temp</l>
                    </list>
                </block>
                <block s="doSetVar">
                    <l>temp</l>
                    <block s="reportListItem">
                        <block var="index1" />
                        <block var="list" />
                    </block>
                </block>
                <block s="doReplaceInList">
                    <block var="index1" />
                    <block var="list" />
                    <block s="reportListItem">
                        <block var="index2" />
                        <block var="list" />
                    </block>
                </block>
                <block s="doReplaceInList">
                    <block var="index2" />
                    <block var="list" />
                    <block var="temp" />
                </block>
            </script>
        </block-definition>
        <block-definition s="new counter" type="reporter" category="operators">
            <header />
            <code />
            <translations />
            <inputs />
            <script>
                <block s="doDeclareVariables">
                    <list><l>count</l></list>
                </block>
                <block s="doSetVar">
                    <l>count</l>
                    <l>0</l>
                </block>
                <block s="doReport">
                    <block s="reifyScript">
                        <script>
                            <block s="doChangeVar">
                                <l>count</l>
                                <l>1</l>
                            </block>
                            <block s="doReport">
                                <block var="count" />
                            </block>
                        </script>
                        <list />
                    </block>
                </block>
            </script>
        </block-definition>
    </blocks>
